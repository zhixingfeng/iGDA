      done

	if test "x$have_gd_fontconfig" = "x"; then
        { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Your libgd was not built with fontconfig support. This may result in problems displaying fonts." >&5
$as_echo "$as_me: WARNING: Your libgd was not built with fontconfig support. This may result in problems displaying fonts." >&2;}
	fi
	if test "x$have_gd_freetype" = "x"; then
	    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Your libgd was not built with freetype support. This may result in problems resolving fonts." >&5
$as_echo "$as_me: WARNING: Your libgd was not built with freetype support. This may result in problems resolving fonts." >&2;}
